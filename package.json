{
  "name": "dex-aggregator-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:registry": "jest services/registry/__tests__",
    "test:cache": "jest services/cache/__tests__",
    "test:network": "jest services/network/__tests__"
  },
  "dependencies": {
    "@polkadot/api": "^15.1.1",
    "@polkadot/types": "^15.1.1",
    "@polkadot/types-augment": "^15.2.1",
    "@polkadot/util": "^13.2.3",
    "@polkadot/util-crypto": "^13.2.3",
    "@substrate/asset-transfer-api": "^0.4.5",
    "@substrate/asset-transfer-api-registry": "^0.2.25",
    "@galacticcouncil/api-augment": "^0.3.0",
		"@galacticcouncil/sdk": "^5.1.0",
		"@galacticcouncil/xcm-cfg": "^6.0.1",
		"@galacticcouncil/xcm-sdk": "^7.0.1",
    "viem": "^2.22.2"
  },
  "packageManager": "pnpm@9.13.0+sha512.beb9e2a803db336c10c9af682b58ad7181ca0fbd0d4119f2b33d5f2582e96d6c0d93c85b23869295b765170fbdaa92890c0da6ada457415039769edf3c959efe",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
